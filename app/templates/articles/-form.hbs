<form {{action "save" on="submit"}}>
  <h2>{{errorMessage}}</h2>
  <fieldset>
    {{input value=model.description placeholder='Description'}}<br><br>
    {{input value=model.notes placeholder='Notes'}}<br><br>
    <input type="submit" value="Save" class='primary'>
    <button {{action "cancel"}}>Cancel</button>
  </fieldset>
</form>
