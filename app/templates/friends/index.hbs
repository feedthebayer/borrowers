<table class='primary'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Articles</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |friend|}}
      <tr>
        <td>{{link-to friend.fullName 'articles' friend}}</td>
        <td>{{friend.totalArticles}}</td>
        <td><small><a href="#" {{action "delete" friend}}>Delete</a></small></td>
      </tr>
    {{/each}}
  </tbody>
</table>
