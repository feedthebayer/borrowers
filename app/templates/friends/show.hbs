<div class='card friend-profile'>
  <div class="friend-info full">
    {{{fill-murray width=300 height=300}}}
    <div>
      <p>{{model.firstName}}</p>
      <p>{{model.lastName}}</p>
      <p>{{model.email}}</p>
      <p>{{model.twitter}}</p>
      {{link-to 'Lend article' 'articles.new'}}
      {{link-to 'Edit info' 'friends.edit' model}}
      <a href="#" {{action "delete" model}}>Delete</a>
    </div>
  </div>
</div>
<div class="articles-container">
  {{outlet}}
</div>
